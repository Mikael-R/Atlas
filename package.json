{
  "name": "bot-admin-discord",
  "version": "1.0.0",
  "description": "Bot to help a manage you server",
  "main": "build/index.js",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpileOnly --ignore-watch node_modules src/index.ts",
    "format": "eslint src --ext ts",
    "build": "rm -rf build && babel src --extensions \".ts\" --out-dir build --copy-files --no-comments",
    "prod": "npm run build && node build/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mikael-R/bot-admin-discord.git"
  },
  "keywords": [],
  "author": "Mikael Rolim de Aquino <mikarg9@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/node": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "babel-plugin-module-resolver": "^4.0.0",
    "discord.js": "^12.2.0",
    "dotenv": "^8.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.0.1",
    "@commitlint/config-conventional": "^9.0.1",
    "@types/cheerio": "^0.22.18",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/jsdom": "^16.2.3",
    "@types/node": "^14.0.14",
    "@types/request": "^2.48.5",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "3.2.0",
    "eslint": "^7.4.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "git-cz": "^4.7.0",
    "husky": "^4.2.5",
    "ts-node-dev": "^1.0.0-pre.50",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format",
      "prepare-commit-msg": "exec < /dev/tty && gitcz --hook || true",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "bugs": {
    "url": "https://github.com/Mikael-R/bot-admin-discord/issues"
  },
  "homepage": "https://github.com/Mikael-R/bot-admin-discord#readme"
}
